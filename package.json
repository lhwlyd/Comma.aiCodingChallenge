{
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "google-map-react": "^1.1.2",
    "mongoose": "^5.4.4",
    "mongoose-double": "^0.0.1",
    "mongoose-geojson-schema": "^2.1.3",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "prop-types": "^15.6.2"
  },
  "scripts": {
    "start:server": "cd backend && nodemon server.js --exec babel-node --presets es2015,stage-0",
    "start:client": "cd client && yarn start",
    "start":"concurrently \"yarn start:server\" \"yarn start:client\"",
    "start:dev":"concurrently \"yarn start:server\" \"yarn start:client\"",
    "heroku-postbuild": "cd client && yarn && yarn start"
  },
  "devDependencies": {}
}
